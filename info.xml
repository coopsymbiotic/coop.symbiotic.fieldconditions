<?xml version="1.0"?>
<extension key="coop.symbiotic.fieldconditions" type="module">
  <file>fieldconditions</file>
  <name>Field Conditions</name>
  <description>Define conditional rules of allowed field values based on the value selected in another field</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Mathieu Lutfy</author>
    <email>mathieu@symbiotic.coop</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://lab.civicrm.org/extensions/fieldconditions/</url>
    <url desc="Documentation">https://lab.civicrm.org/extensions/fieldconditions/</url>
    <url desc="Support">https://lab.civicrm.org/extensions/fieldconditions/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-11-21</releaseDate>
  <version>1.8.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.73</ver>
  </compatibility>
  <comments/>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
    <psr0 prefix="CRM_" path="."/>
  </classloader>
  <civix>
    <namespace>CRM/Fieldconditions</namespace>
    <format>24.09.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>entity-types-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
  </mixins>
  <upgrader>CRM_Fieldconditions_Upgrader</upgrader>
</extension>
